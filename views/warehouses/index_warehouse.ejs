<h2>Search Warehouse</h2>
<form action="/warehouses" method="GET"> 
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% warehouses.forEach(warehouse => { %>
    <div><%= warehouse.name %></div>
<% }) %> 